#define PB push_back
#define FOR(aa,nn) for(aa=0; aa<nn; aa++)


using namespace std;
LL a[100001],b[100001];
int n, m;

LL solve(LL num)
{
    LL ret=0;
    for(int i=0; i<n; i++)
    {
        if(a[i]>num) break;
        ret+=llabs(num-a[i]);
    }

    for(int i=m; i>=0; i--)
    {
        if(a[i]<=num) break;
        ret+=llabs(num-b[i]);
    }
    return ret;
}


LL BS()
{
    LL st = min(a[0], b[0]);
    LL ed = max(a[n-1], b[m-1]);
    LL mid=(st+ed)/2;
    int cnt=0;
    LL mn=1000000000000000;
    while(st<=ed)
    {
        mid =(st+ed)/2;
        LL need= solve(mid);
        if(need<=mn)
            ed=mid;
        else
            st=mid+1;
        mn = min(mn, need);
         cnt++;
         //cout<<cnt<<endl;
         if(cnt>=110)break;

    }
    return ed;
}


int main()
{
    //   FILE * fin, * fout, *fp;
//     fp=fopen("out.txt", "w");

    int i,j;

    cin>>n>>m;
    for(i=0; i<n; i++)
        cin>>a[i];
    for(i=0; i<m; i++)
        cin>>b[i];
    sort(a,a+n);
    sort(b,b+m);

    LL res = BS();
    cout<<res<<endl;


    return 0;
}

