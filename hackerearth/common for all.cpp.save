#include <bits/stdc++.h>

#define LL long long
#define MX 5000+7
#define MOD 1000000007
#define N 1000007
#define check_bit(a,b) (a & (1<<b))
#define PB push_back
#define FOR(aa,nn) for(aa=0; aa<nn; aa++)


using namespace std;



LL arr[100001];
LL sum[100001];

LL P, Q;
LL BS(LL A, LL B)
{
    LL st = A, ed = B, mid = (A+B)/2;
    int cnt = 0;
    while(st<ed)
    {
        mid = (st+ed)/2;
        if(mid>Q)
            ed = mid - 1;
        else if(mid<P)
            st = mid + 1;
        else
        {
            LL a = llabs(mid - A);
            LL b = llabs(mid - B);
            if(a==b) return mid;
            if(a>b)
            {
                if(P<=(mid - 1) && (mid-1)<=Q)
                    ed = mid - 1;
                else
                    st = mid ;
            }
            else
            {
                if(P<=(mid+1) && (mid+1)<=Q)
                    st = mid + 1;
                else
                    ed = mid ;
            }
        }
        cnt++;
        if(cnt==100)
        {
            if(P<=(st) && (st)<=Q)
                return st;
            return ed;
        }

    }
    return ed;
}

